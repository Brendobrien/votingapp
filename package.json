    "babel-preset-react-native": "^4.0.0",
    "exp": "^51.1.0",
    "flow-bin": "^0.56.0",
    "husky": "^0.13.3",
    "jest-expo": "24.0.0",
    "lint-staged": "^3.5.0",
    "prettier": "^1.9.2",
    "react-hot-loader": "^3.1.3",
    "react-native-scripts": "1.8.1",
    "react-test-renderer": "16.0.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
